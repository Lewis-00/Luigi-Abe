{
  "printWidth": 100,
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro"
      }
    },
    {
      "files": ["*.js", "*.jsx", "*.ts", "*.tsx", "*.json", "*.css"],
      "options": {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    }
  ]
}
